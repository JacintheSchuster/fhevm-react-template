{
  "name": "@zama/fhevm-sdk",
  "version": "1.0.0",
  "description": "Universal, framework-agnostic SDK for building confidential frontends with Zama FHEVM",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "require": "./dist/react/index.js",
      "import": "./dist/react/index.mjs",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
      "require": "./dist/vue/index.js",
      "import": "./dist/vue/index.mjs",
      "types": "./dist/vue/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fhevm",
    "zama",
    "encryption",
    "fhe",
    "privacy",
    "confidential",
    "blockchain",
    "ethereum",
    "sdk",
    "react",
    "vue",
    "nextjs",
    "typescript"
  ],
  "author": "Universal FHEVM SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/fhevm-react-template"
  },
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/fhevm-react-template/issues"
  },
  "homepage": "https://github.com/YOUR_USERNAME/fhevm-react-template#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.6.0",
    "@zama-fhe/gateway-sdk": "^0.2.0",
    "ethers": "^6.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "react": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0"
  }
}
